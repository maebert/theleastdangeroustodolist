{
  "expo": {
    "name": "The Least Dangerous To-Do List",
    "scheme": "tldtdl",
    "slug": "theleastdangeroustodolist",
    "privacy": "public",
    "platforms": ["ios"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#464152"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "15",
      "bundleIdentifier": "me.1450.tldtdl",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "sdkVersion": "38.0.0",
    "android": {
      "package": "com.theleastdangeroustodolist.app"
    },
    "entryPoint": "node_modules/expo/AppEntry.js"
  }
}
